class Solution {
    public int removeElement(int[] nums, int val) {
        int newNumsCount = 0;
        for(int i =0; i<nums.length; i++)
        {
            if(nums[i] != val)
            {
                nums[newNumsCount] = nums[i];
                newNumsCount++;
            }
        }

        return newNumsCount;
    }
}
